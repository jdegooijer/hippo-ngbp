    "angular-ui-router": "0.3.1"
  },
  "devDependencies": {
    "angular-mocks": "1.5.6",
    "autoprefixer": "6.4.1",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.4.2",
    "eslint-plugin-react": "5.1.1",
